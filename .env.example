# MediaFlow Proxy Configuration Example

# API password for protecting endpoints
# api_password=your_secure_password_here

# Logging level (DEBUG, INFO, WARNING, ERROR)
# log_level=INFO

# Disable SSL verification for HLS proxy endpoints
# Set to true to resolve SSL certificate verification errors
# WARNING: This reduces security - only use if you trust the network
# disable_ssl_verification_for_hls=true

# Enable streaming progress tracking
# enable_streaming_progress=false

# Disable home page UI
# disable_home_page=false

# Disable API documentation (Swagger UI)
# disable_docs=false

# Disable speedtest UI
# disable_speedtest=false

# User agent for HTTP requests
# user_agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/136.0.0.0 Safari/537.36

# HLS pre-buffering settings
# enable_hls_prebuffer=false
# hls_prebuffer_segments=5
# hls_prebuffer_cache_size=50

# DASH pre-buffering settings
# enable_dash_prebuffer=false
# dash_prebuffer_segments=5
# dash_prebuffer_cache_size=50

# Transport configuration
# proxy_url=socks5://user:pass@proxy:1080
# all_proxy=false
# timeout=30
